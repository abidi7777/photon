{
  "name": "photon",
  "version": "1.0.0",
  "description": "A simple Browser based Photo editor using CSS filters",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dev:host": "vite --host",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "test --watch",
    "js:lint": "eslint ./src --ext .js,.svelte",
    "style:lint": "stylelint ./src/**/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abidi7777/photon.git"
  },
  "keywords": [
    "photo",
    "editor",
    "css",
    "filter"
  ],
  "author": "abidi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abidi7777/photon/issues"
  },
  "homepage": "https://github.com/abidi7777/photon#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@vitejs/plugin-legacy": "^1.8.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.1",
    "cssnano": "^5.1.12",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "postcss": "^8.4.14",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "svelte-jester": "^2.3.2",
    "tailwindcss": "^3.1.4",
    "vite": "^2.9.12"
  },
  "dependencies": {
    "browser-image-compression": "^2.0.0",
    "lewk": "^1.1.1",
    "svelte": "^3.48.0",
    "svelte-range-slider-pips": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
